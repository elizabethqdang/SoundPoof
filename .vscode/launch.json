{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [

	
		{
			"type": "chrome",
			"request": "attach",
			"name": "Attach to Chrome port 9222 workspaceFolder",
			"port": 9222,
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach node port 9229 workspaceFolder",
			"port": 9229,
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against 3000 workspaceFolder",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "devtools-for-chrome",
			"request": "attach",
			"name": "Attach Chrome DevTools 3000",
			"file": "http://localhost:3000"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Local Node File",
			"console": "integratedTerminal",
			"program": "{workspaceFolder}"
		}
	]
}